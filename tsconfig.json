/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "cats": [
        "dist/cats/cats",
        "dist/cats"
      ],
      "navigation": [
        "dist/navigation/navigation",
        "dist/navigation"
      ],
      "pracownicy": [
        "dist/pracownicy/pracownicy",
        "dist/pracownicy"
      ],
      "team": [
        "dist/team/team",
        "dist/team"
      ],
      "@employees": ["project/employees/src/public-ati.ts"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false
  }
}
